{
    "name": "unisharp/laravel-audit-trail2",
    "description": "Keep a change history for your models using laravel version 5.5.*",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "UniSharp Ltd.",
            "email": "unisharp-service@unisharp.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "~5.5",
        "illuminate/database": "~5.5",
        "illuminate/events": "~5.5",
        "illuminate/http": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Unisharp\\AuditTrail2\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests"
      },
      "classmap": [
          "tests/migrations"
      ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Unisharp\\AuditTrail2\\AuditServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
